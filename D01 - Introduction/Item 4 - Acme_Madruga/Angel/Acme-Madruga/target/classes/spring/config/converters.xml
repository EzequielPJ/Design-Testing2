<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">

		<!-- Authority -->
		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.StringToAuthorityConverter" />

		<!-- Area -->
		<bean class="converters.AreaToStringConverter" />
		<bean class="converters.StringToAreaConverter" />

		<!-- Box -->
		<bean class="converters.BoxToStringConverter" />
		<bean class="converters.StringToBoxConverter" />

		<!-- Position -->
		<bean class="converters.PositionToStringConverter" />
		<bean class="converters.StringToPositionConverter" />

		<!-- Profile -->
		<bean class="converters.ProfileToStringConverter" />
		<bean class="converters.StringToProfileConverter" />

		<!-- Procession -->
		<bean class="converters.ProcessionToStringConverter" />
		<bean class="converters.StringToProcessionConverter" />

		<!-- Actor -->
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />

		<!-- Message -->
		<bean class="converters.MessageEntityToStringConverter" />
		<bean class="converters.StringToMessageEntityConverter" />
		
		<!-- Enrolment -->
		<bean class="converters.EnrolmentEntityToStringConverter" />
		<bean class="converters.StringToEnrolmentEntityConverter" />
		
		<!-- Float -->
		<bean class="converters.FloatEntityToStringConverter" />
		<bean class="converters.StringToFloatEntityConverter" />

		<!-- Brotherhood -->
		<bean class="converters.BrotherhoodEntityToStringConverter" />
		<bean class="converters.StringToBrotherhoodEntityConverter" />


	</util:list>

</beans>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- Authority -->
		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.StringToAuthorityConverter" />
		<!-- Administrator -->
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.StringToAdministratorConverter" />
		<!-- CreditCard -->
		<bean class="converters.CreditCardToStringConverter" />
		<bean class="converters.StringToCreditCardConverter" />
		<!-- Profile -->
		<bean class="converters.ProfileToStringConverter" />
		<bean class="converters.StringToProfileConverter" />
		<!-- Ticker -->
		<bean class="converters.TickerToStringConverter" />
		<bean class="converters.StringToTickerConverter" />
		<!-- Message -->
		<bean class="converters.MessageEntityToStringConverter" />
		<bean class="converters.StringToMessageEntityConverter" />
		<!-- Actor -->
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		<!-- Sponsorship -->
		<bean class="converters.SponsorshipToStringConverter" />
		<bean class="converters.StringToSponsorshipConverter" />
		<!-- Box -->
		<bean class="converters.BoxToStringConverter" />
		<bean class="converters.StringToBoxConverter" />
		<!-- Category -->
		<bean class="converters.CategoryToStringConverter" />
		<bean class="converters.StringToCategoryConverter" />
		<!-- Swap -->
		<bean class="converters.SwapToStringConverter" />
		<bean class="converters.StringToSwapConverter" />
		<!-- Collaborator -->
		<bean class="converters.CollaboratorToStringConverter" />
		<bean class="converters.StringToCollaboratorConverter" />
		<!-- Event -->
		<bean class="converters.EventToStringConverter" />
		<bean class="converters.StringToEventConverter" />
		<!-- MiscellaneousReport -->
		<bean class="converters.MiscellaneousReportToStringConverter" />
		<bean class="converters.StringToMiscellaneousReportConverter" />
		<!-- Notes -->
		<bean class="converters.NotesToStringConverter" />
		<bean class="converters.StringToNotesConverter" />
		<!-- Comission -->
		<bean class="converters.ComissionToStringConverter" />
		<bean class="converters.StringToComissionConverter" />
		<!-- Portfolio -->
		<bean class="converters.PortfolioToStringConverter" />
		<bean class="converters.StringToPortfolioConverter" />
		<!-- Sponsor -->
		<bean class="converters.SponsorToStringConverter" />
		<bean class="converters.StringToSponsorConverter" />
		<!-- Comment -->
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.StringToCommentConverter" />
		<!-- Proclaim -->
		<bean class="converters.ProclaimToStringConverter" />
		<bean class="converters.StringToProclaimConverter" />
		<!-- Student -->
		<bean class="converters.StudentToStringConverter" />
		<bean class="converters.StringToStudentConverter" />
		<!-- Customisation -->
		<bean class="converters.CustomizationToStringConverter" />
		<bean class="converters.StringToCustomisationConverter" />
	</util:list>

</beans>
		